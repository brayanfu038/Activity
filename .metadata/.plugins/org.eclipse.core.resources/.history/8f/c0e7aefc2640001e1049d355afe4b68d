package model;

import java.util.HashSet;
import java.util.Iterator;
import java.util.Scanner;

public class SubSet {
    public static void main(String[] args) {
        Scanner console = new Scanner(System.in);
        String conjunto = "{1,2,3}";

        // Extracci√≥n de los elementos individuales del conjunto
        String subcadena = conjunto.substring(1, conjunto.length() - 1);
        String[] cadena1 = subcadena.split(",");
        HashSet<Integer> input = new HashSet<Integer>();

        // pasamos a enteros los numeros y los agregamos al conjunto S
        for (int i = 0; i < cadena1.length; i++) {
            input.add(Integer.parseInt(cadena1[i]));
        }
        
        HashSet<Integer>SubConjunt = new HashSet<Integer>(new Hash<Integer>);
        
        

 
    }
}
